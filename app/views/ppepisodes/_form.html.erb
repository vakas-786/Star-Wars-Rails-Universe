<% if flash[:my_errors] %>
<% flash[:my_errors].each do |e| %>
  <%= e %>
<br>
<% end %>
<% end %>

<%= form_for @ppepisode do |f| %> 
  <%= f.label :episode_id %>
  <%= f.collection_select :episode_id, @episodes, :id, :title %>
  <%= f.label :person_id %>
  <%= f.collection_select :person_id, @people, :id, :name %>
  <%= f.label :planet_id %>
  <%= f.collection_select :planet_id, @planets, :id, :name %>
<%= f.submit "Build Universe" %> 
<% end %>